doctype xml
svg(
  xmlns='http://www.w3.org/2000/svg'
  width='200'
  height='200'
)
  style.
    .rect{fill:blue;}
  g(transform='translate(100,100)')
    for i in [0,1,2,3,4,5,6,7]
      g(transform='rotate('+(i*45)+'),translate(68),rotate(45)')
        rect(x='-20',y='-20',width='40',height='40',class='rect')

